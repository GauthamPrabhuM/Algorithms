//insertion sort 
//cards in left hand picked and sorted in right hand

//O(n^2)


#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i=0; i<n; i++)
	cin>>arr[i];
	/* for j=2 to A.length
	key=A[j]
	insert A[j] into sorted sequence
	i=j-1
	while i>0 and A[i]>key
	 A[i+1]=A[i--]
   A[i+1]=key */
   
   
	for(int i=1; i<n; i++)
	{
	    int current=arr[i];
	    int j=i-1;
	    while(arr[j]>current&&j>=0)
	    {
	        
	        arr[j+1]=arr[j--];
	    }
	    arr[j+1]=current;
	    
	}
	for(int i=0; i<n; i++)
	cout<<arr[i];
	
	return 0;
}
